{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "StrictMode": true,
    "ProvisionalAuthentucationMethods": "SAS",
    "PermanentAuthentucationMethods": "X509",
    "GlobalPatterns": [],
    "FilesRestrictions": [
      {
        "Id": "LogUploadAllow",
        "Type": "Upload",
        "Root": "c:/demo/",
        "allowPatterns": [
          "**/*.log",
          "*/*.png",
          "*.txt"
        ],
        "DenyPatterns": []
      },
      {
        "Id": "ImageUploadAllow",
        "Type": "Upload",
        "Root": "c:/demo/images/",
        "allowPatterns": [
          "/sub/*",
          "*.jpg"
        ],
        "DenyPatterns": []
      },
      {
        "Id": "Demo",
        "Type": "Download",
        "Root": "c:/demo/",
        "MaxSize": 1,
        "AllowPatterns": [
          "*/demo/*.jpg"
        ],
        "DenyPatterns": []
      }
    ]
  }
}