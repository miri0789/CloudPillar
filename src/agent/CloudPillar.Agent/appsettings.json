      {
        "Id": "ImageUploadAllow",
        "Type": "Upload",
        "filePath": "c:/demo/images/test.txt",
        "allowPatterns": [
          "/sub/*",
          "*.jpg"
        ],
        "DenyPatterns": []
      },
      {
        "Id": "Demo",
        "Type": "Download",
        "Root": "c:/demo/",
        "MaxSize": 1,
        "AllowPatterns": [
          "*/demo/*.jpg"
        ],
        "DenyPatterns": []
      }
    ]
  },
