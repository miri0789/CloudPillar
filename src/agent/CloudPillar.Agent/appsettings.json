{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "StrictMode": true,
    "ProvisionalAuthentucationMethods": "SAS",
    "PermanentAuthentucationMethods": "X509",
    "GlobalPatterns": [
      "**/*.log",
      "*.txt"
    ],
    "FilesRestrictions": [
      {
        "Id": "LogUploadAllow",
        "Type": "Upload",
        "Root": "c:/demo/",
        "allowPatterns": [],
        "DenyPatterns": []
      },
      {
        "Id": "Demo",
        "Type": "Download",
        "Root": "c:/demo/",
        "MaxSize": 99999,
        "AllowPatterns": [
          "*/demo/*.jpg",
          "demo/stream2w.jpg"
        ],
        "DenyPatterns": []
      }
    ]
  }
}