    "ProvisionalAuthenticationMethods": "SAS",
    "PermanentAuthenticationMethods": "X509",    
    "GlobalPatterns": [
      "**/test/*.txt"
    ],
    "FilesRestrictions": [
      {
        "Id": "LogUploadAllow",
        "Type": "Upload",
        "Root": "c:/demo/",
        "allowPatterns": [
          "**/*.log",
          "*/*.png",
          "*.txt"
        ],
        "DenyPatterns": []
